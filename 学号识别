#include<stdio.h>
int main()
{
    int a,b,c,d,e,n;
    scanf("%d",&n);
    e=n%10+((n/10)%10)*10;
    d=(n/100)%10+((n/1000)%10)*10;
    c=(n/10000)%10+((n/100000)%10)*10;
    b=(n/1000000)%10+((n/10000000)%10)*10;
    a=(n/100000000)%10+((n/1000000000)%10)*10;
    printf("Grade %02d,College %02d,Major %02d,Class %02d,Number %02d",a,b,c,d,e);
    return 0;
}

