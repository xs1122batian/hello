#include<stdio.h>
int main()
{
    int a,b,c,d,e,f,g,h;
    while(scanf("%d",&a)&&(a!=0))
     {f=0;
     c=0;
         for(d=1;d<=a;d++)
     {
        for(g=d;g!=0;)
     {
        b=g%10;
        g=g/10;
        c=c*10+b;}
        if(c==d)
        {f=f+1;}
        c=0;
        }
        printf("%d\n",f);}
        return 0;

}
