#include<stdio.h>
int main()
{
    int a,b,c,d,n;
    a=3;
    b=2;
    scanf("%d",&n);
    if(n==1)
        printf("2/1\n");
    else if(n==2)
        printf("3/2\n");
    else
        {for(d=3;d<=n;d++)
    {
        c=a;
        a=a+b;
        b=c;
    }
    printf("%d/%d",a,b);}
    return 0;
}
