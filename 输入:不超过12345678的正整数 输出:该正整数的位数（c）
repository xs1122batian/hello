#include<stdio.h>
int main( )
{ 
int a,b;
scanf( "%d",&a);
if( a>0&&a<=9)
{ b=1;}
else if( a>=10&&a<=99)
{ b=2;}
else if(a>=100&&a<=999 )
{ b=3;}
else if( a>=1000&&a<=9999)
{ b=4;}
else if( a>=10000&&a<=99999)
{ b=5;}
else if( a>=100000&&a<=999999)
{ b=6;}
else if( a>=1000000&&a<=9999999)
{ b=7;}
else if( a>=10000000&&a<=99999999)
{ b=8;}
printf( "%d",b);
return 0;
}
