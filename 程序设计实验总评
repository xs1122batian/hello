#include<stdio.h>
int main()
{
    int a,b,c,d;
    double sum;
    scanf("%d%d%d",&a,&b,&c);
    if(a<0||a>100||b<0||b>100||c<0||c>100)
        d=0;
    else
    {
        sum=a*0.1+b*0.2+c*0.7;
        d=1;
    }
    if(d==0)
        printf("Input Error!\n");
    else
    {
        if(sum<60)
            printf("Sorry! You failed the exam!\n");
        else
            printf("Congratulations! You passed the exam!\n");
    }
    return 0;
}
