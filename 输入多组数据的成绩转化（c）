#include<stdio.h>
int main()
{
    int a,b;
    while(scanf("%d",&a)!=EOF)
        if(a>100||a<0)
        printf("Score is error!");
    else{
    b=a/10;
    switch(b)
{
        case  9:
        case 10:printf("A\n");break;
        case 8:printf("B\n");break;
        case 7:printf("C\n");break;
        case 6:printf("D\n");break;
        default:printf("E\n");break;
   }
    }
    return 0;
}
