#include<stdio.h>
#include<math.h>
int main( )
{ 
double x,y;
scanf( "%lf",&x);
if( x<0)
{ y=-( x/2);
printf( "y=%f",y);}
else if(x<10&&x>=0)
{ y=exp( x) +3;
printf( "y=%f",y);}
else if(x>=10&&x<20)
{ y=log( x);
printf("y=%f",y );}
else if(x>= 20&&x<30)
{y= pow( x,1.5);
printf( "y=%f",y);}
else if( x>=30&&x<50)
{ y=sqrt( x)-1;
printf( "y=%f",y);}
else if( x>=50)
{ y=cos( x);
printf( "%f",y);}
return 0;
}
