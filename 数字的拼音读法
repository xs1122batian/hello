#include<stdio.h>
int main()
{
    int a,b,c,d,e,n;
    b=0;
    c=0;
    scanf("%d",&n);
       if(n==0)
        printf("ling ");
    else if(n>0)
    {while(n)
    {
        a=n%10;
        b=b*10+a;
        n=n/10;
        c++;
    }
    for(d=0;d<c;d++)
    {
        e=b%10;
        b=b/10;
        if(e==0)
            printf("ling ");
        else if(e==1)
            printf("yi ");
        else if(e==2)
            printf("er ");
        else if(e==3)
            printf("san ");
        else if(e==4)
            printf("si ");
        else if(e==5)
            printf("wu ");
        else if(e==6)
            printf("liu ");
        else if(e==7)
            printf("qi ");
        else if(e==8)
            printf("ba ");
        else if(e==9)
            printf("jiu ");
    }}
    else if(n<0)
    {n=-n;
    printf("fu ");
        while(n)
    {
        a=n%10;
        b=b*10+a;
        n=n/10;
        c++;
    }
    for(d=0;d<c;d++)
    {
        e=b%10;
        b=b/10;
        if(e==0)
            printf("ling ");
        else if(e==1)
            printf("yi ");
        else if(e==2)
            printf("er ");
        else if(e==3)
            printf("san ");
        else if(e==4)
            printf("si ");
        else if(e==5)
            printf("wu ");
        else if(e==6)
            printf("liu ");
        else if(e==7)
            printf("qi ");
        else if(e==8)
            printf("ba ");
        else if(e==9)
            printf("jiu ");}}
    return 0;
}

