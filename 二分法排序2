int main()
{
    int DB[]={2,5,7,9};
    int left,right,mid,d;
    left=0;
    right=3;
    mid=0;
    while(left<right)
    {
        mid=(left+right)/2;
        if(DB[mid]<6)
        left=mid+1;
        else if(DB[mid]>6)
        right=mid-1;
        else if(DB[mid]=6)
        {
           d=1;
           break;
        }
    }
    if(d==1)
        printf("有6这个元素");
        else
        printf("没有6这个元素");
    return 0;
}

